#include<bits/stdc++.h>

#define MAXN 60000
#define MAXDEPTH 40

using namespace std;

int par[MAXN] = {0};
int depth[MAXDEPTH] = {0};
string key[MAXN] = {0};

void init(int n)
{
  for(int i = 0; i < n; i++) {
    par[i] = i;
    depth[i] = 0;
  }
}

int findroot(int x)
{
  if(par[x] == x) {
    return x;
  } else {
    return par[x] = find(par[x]);
  }
}

